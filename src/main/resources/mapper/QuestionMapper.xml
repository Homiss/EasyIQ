<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wcode.mapper.QuestionMapper">
  <resultMap id="BaseResultMap" type="cn.wcode.model.Question">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="q_group_id" jdbcType="INTEGER" property="qGroupId" />
    <result column="question" jdbcType="VARCHAR" property="question" />
    <result column="answer" jdbcType="VARCHAR" property="answer" />
  </resultMap>
  <select id="getByQuestionGroupId" resultType="cn.wcode.model.Question">
    select * from question where q_group_id = #{qGroupId}
  </select>

</mapper>